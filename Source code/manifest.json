{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Snapsurf",
    "version": "1.0",
    "description": "Unleash speed with SnapSurf. Speedup your browsing in a snap!",
    "manifest_version": 3,
    "author": "Arpan Pramanik",
     "action": {
        "default_title": "Snapsurf",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "files/logo16.png",
        "32": "files/logo32.png",
        "48": "files/logo48.png",
        "64": "files/logo64.png",
        "128": "files/logo128.png"
    },
   
    "permissions": [
        "scripting","activeTab","storage", "browsingData", "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}